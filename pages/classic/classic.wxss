/* pages/classic/classic.wxss */
/* view 默认是100% */
.container {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.header {
  width: 100%;
  display: flex;
  flex-direction: row;
  height: 100rpx;
  align-items: center;
  justify-content: space-between;
  border-top: 1px solid #f5f5f5;
  border-bottom: 1px solid #f5f5f5;
}

.like-model {
  margin-top: 6rpx;
  margin-right: 25rpx;
}

.navi {
  position: absolute;
  bottom: 40rpx;
}

.like-container{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin-right:30rpx;
}


.share-btn {
  margin-top: 5rpx;
  margin-left: 10rpx;
}

.share {
  width: 40rpx;
  height: 40rpx;
}